            <field name="inherit_id" ref="account.view_move_form"/><!-- module.ref_id(tree/form) -->
            <field name="arch" type="xml">
                <xpath expr="//field[@name='invoice_payment_term_id']" position="after"><!-- after/before/inside/attributes -->
                    <field name="efgh"/>
                </xpath>
            </field>
        </record>

        <record id="view_invoice_cust5" model="ir.ui.view">
            <field name="name">account.move.form.cust</field>
            <field name="model">account.move</field>
