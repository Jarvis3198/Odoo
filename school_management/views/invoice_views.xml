<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        

        <record id="view_invoice_cust" model="ir.ui.view">
            <field name="name">account.move.form.cust</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/><!-- module.ref_id(tree/form) -->
            <field name="arch" type="xml">
                <xpath expr="//field[@name='invoice_date']" position="after"><!-- after/before/inside/attributes -->
                    <field name="abcd"/>
                </xpath>
            </field>
        </record>

        <record id="view_invoice_cust2" model="ir.ui.view">
            <field name="name">account.move.form.cust</field>
